<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" >
  
  <Import Project="nuget.targets" />
  
  <PropertyGroup>
    <BuildTargets>
      $(BuildTargets);
      NugetBuild;
    </BuildTargets>
    <CleanTargets>
      $(CleanTargets);
      NugetClean;
    </CleanTargets>
    <RebuildTargets>
      $(RebuildTargets);
      NugetRebuild;
    </RebuildTargets>
    <PublishTargets>
      $(PublishTargets);
      NugetPublish;
    </PublishTargets>
  </PropertyGroup>

</Project>
